<mapper namespace="org.apache.ibatis.submitted.rounding.Mapper">
    <!-- 填入内容需要按照下面例子改写sql-->
</mapper>

sql example 1
<mapper namespace="com.ideal.springbootweb.mapper.UserMapper">
    <select id="findAll" resultType="com.ideal.springbootweb.entity.User">
        select * from user
    </select>
</mapper>
